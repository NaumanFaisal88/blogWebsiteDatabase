<%- include("partials/header"); -%>
    <h1>
        Compose
    </h1>
    <form class="write" action="/compose" method="post">
        <div class="form-group">
            <label for="input1">Title</label>
            <input type="text" name="titleInput" class="form-control" id="input1">
        </div>
        <div class="form-group">
            <label for="input2">Description</label>
            <textarea type="text" name="contentInput" class="form-control" id="input2" rows="10"></textarea>
        </div>
        <button type="submit" class="btn btn-primary">Publish</button>
    </form>

    <%- include("partials/footer"); -%>